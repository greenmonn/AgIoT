<body class="fix-header">
<!-- ============================================================== -->
<!-- Preloader -->
<!-- ============================================================== -->
<div class="preloader">
    <svg class="circular" viewBox="25 25 50 50">
        <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
    </svg>
</div>
<!-- ============================================================== -->
<!-- Wrapper -->
<!-- ============================================================== -->
<div id="wrapper">
    <!-- ============================================================== -->
    <!-- Topbar header - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <nav class="navbar navbar-default navbar-static-top m-b-0">
        <div class="navbar-header">
            <div>
                <!-- Logo -->
                <a class="logo" href="/new">
                    <!-- Logo icon image, you can use font-icon also --><b>
                        <img src="./images/farm_logo.png" alt="home" class="light-logo" />
                    </b>
                    <!--&lt;!&ndash; Logo text image you can use text also &ndash;&gt;<span class="hidden-xs">-->
                        <!--&lt;!&ndash;This is dark logo text&ndash;&gt;<img src="/images/farm_logo.png" alt="home" class="dark-logo" />&lt;!&ndash;This is light logo text&ndash;&gt;<img src="/images/farm_logo.png" alt="home" class="light-logo" />-->
                     <!--</span> --></a>
            </div>
            <!-- /Logo -->
            <!-- Search input and Toggle icon -->
            <ul class="nav navbar-top-links navbar-right pull-right">
                <li class="dropdown">
                    <a class="dropdown-toggle profile-pic"> <img src="./images/profile.png" alt="user-img" width="36" class="img-circle"><b class="hidden-xs">happyfarm1234</b> </a>
                    <ul class="dropdown-menu dropdown-user animated flipInY">
                        <li>
                            <div class="dw-user-box">
                                <div class="u-img"><img src="./images/profile.png" alt="user" /></div>
                                <div class="u-text">
                                    <h4>Steave Jobs</h4>
                                    <p class="text-muted">varun@gmail.com</p><a href="profile.html" class="btn btn-rounded btn-danger btn-sm">View Profile</a></div>
                            </div>
                        </li>
                        <li role="separator" class="divider"></li>
                        <li><a href="javascript:void(0)"><i class="ti-user"></i> My Profile</a></li>
                        <li><a href="javascript:void(0)"><i class="ti-wallet"></i> My Balance</a></li>
                        <li><a href="javascript:void(0)"><i class="ti-email"></i> Inbox</a></li>
                        <li role="separator" class="divider"></li>
                        <li><a href="javascript:void(0)"><i class="ti-settings"></i> Account Setting</a></li>
                        <li role="separator" class="divider"></li>
                        <li><a href="javascript:void(0)"><i class="fa fa-power-off"></i> Logout</a></li>
                    </ul>
                    <!-- /.dropdown-user -->
                </li>
                <!-- /.dropdown -->
            </ul>
        </div>
    </nav>
    <div id="page-wrapper" style="padding-top: 0">
        <div class="container-fluid">
            <div class="row bg-title">
                <div class="col-lg-3 col-md-12 col-sm-12 col-xs-12">
                    <h4 class="page-title"><span class="">Image Streaming Service</span> Demo</h4>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- ============================================================== -->
            <!-- Sales, finance & Expance widgets -->
            <!-- ============================================================== -->
            <!-- .row -->
            <div class="row">
                <div class="col-md-12 col-lg-6 col-sm-12">
                    <div class="white-box">
                        <h3 class="box-title">NEWLY UPDATED IMAGE</h3>
                            <table align="center">
                                <% for(var i = 0; i < num_row; i++) { %>
                                <tr>
                                    <% for (var j = 0; j < num_col; j++) { %>
                                    <% var number = i * num_col + j; %>

                                    <td id="cell-<%= number %>"
                                            <% if (imageCells[number]) { %>
                                        style="background-image: url(<%= imageCells[number] %>)"
                                            <% } %>><div></div>
                                    </td>

                                    <% } %>
                                </tr>
                                <% } %>
                            </table>
                    </div>
                </div>
                <div class="col-md-12 col-lg-6 col-sm-12">
                    <div class="white-box bg-theme-alt m-b-0">
                        <h3 class="box-title text-white">LOG</h3>
                        <div class="demo-container" style="height:140px;">
                            <div id="placeholder" class="demo-placeholder"></div>
                        </div>
                    </div>
                    <div class="white-box p-b-0">
                        <div class="row">
                            <div class="col-xs-8">
                                <h2 class="font-medium m-t-0"><span id="average_update_period">0</span> ms</h2>
                                <h5 class="text-muted m-t-0">Average Update Period of a Patch</h5>
                            </div>
                            <div class="col-xs-4">
                                <div class="circle-md pull-right circle bg-info"><i class="ti-plus"></i></div>
                            </div>
                        </div>
                        <div class="row m-t-30 minus-margin">
                            <div class="col-sm-12 col-sm-6 b-t b-r">
                                <ul class="expense-box">
                                    <li><i class="ti-view-grid text-info"></i>
                                        <div>
                                            <h2><span id="changed_cell_number"> - </span></h2>
                                            <h4>Changed Patch Number</h4>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                            <div class="col-sm-12 col-sm-6  b-t">
                                <ul class="expense-box">
                                    <li><i class="ti-pulse text-info"></i>
                                        <div>
                                            <h2><span id="update_period"> 0 </span> ms</h2>
                                            <h4>Update Period</h4>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div class="row minus-margin">
                            <div class="col-sm-12 col-sm-6  b-t b-r">
                                <ul class="expense-box">
                                    <li><i class="ti-layout-grid4-alt text-info"></i>
                                        <div>
                                            <h2><span id="update_count"> 0 </span> </h2>
                                            <h4>Total Changed Patch</h4>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                            <div class="col-sm-12 col-sm-6  b-t">
                                <ul class="expense-box">
                                    <li><i class="ti-timer text-info"></i>
                                        <div>
                                            <h2><span id="sum_update_period"> 0 </span> ms</h2>
                                            <h4>Total Update Period</h4>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- col-md-3 -->
                <div class="col-md-6 col-lg-3" style="display:none">
                    <div class="white-box">
                        <h3 class="box-title">Finance</h3>
                        <div id="diagram"></div>
                        <div class="get">
                            <div class="arc"> <span class="text">Aug</span>
                                <input type="hidden" class="percent" value="95" />
                                <input type="hidden" class="color" value="#53e69d" /> </div>
                            <div class="arc"> <span class="text">Sep</span>
                                <input type="hidden" class="percent" value="90" />
                                <input type="hidden" class="color" value="#ff7676" /> </div>
                            <div class="arc"> <span class="text">Oct</span>
                                <input type="hidden" class="percent" value="80" />
                                <input type="hidden" class="color" value="#88B8E6" /> </div>
                            <div class="arc"> <span class="text">Nov</span>
                                <input type="hidden" class="percent" value="53" />
                                <input type="hidden" class="color" value="#BEDBE9" /> </div>
                            <div class="arc"> <span class="text">Dec</span>
                                <input type="hidden" class="percent" value="45" />
                                <input type="hidden" class="color" value="#EDEBEE" /> </div>
                        </div>
                        <div class="row p-t-30">
                            <div class="col-xs-8">
                                <h1 class="font-medium m-t-0">56%</h1>
                                <h5 class="text-muted m-t-0">increase in Nov</h5>
                            </div>
                            <div class="col-xs-4">
                                <div class="circle-md pull-right circle bg-success"><i class="ti-stats-up"></i></div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- col-md-3 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
        <footer class="footer text-center"> 2018 &copy; K-SW Project by team Atoi </footer>
    </div>
    <!-- /#page-wrapper -->
</div>
<!-- /#wrapper -->
<script src="/socket.io/socket.io.js"></script>
